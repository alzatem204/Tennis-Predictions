ps://localhost:8080/"},"id":"DdcYHpGkW09M","executionInfo":{"status":"ok","timestamp":1751748672890,"user_tz":300,"elapsed":45,"user":{"displayName":"Juan José Alzate Molina","userId":"13371847134369819030"}},"outputId":"85a53abb-c56d-441b-951e-92e86767586b"},"execution_count":6,"outputs":[{"output_type":"stream","name":"stderr","text":["/tmp/ipython-input-6-2095498531.py:10: SettingWithCopyWarning: \n","A value is trying to be set on a copy of a slice from a DataFrame.\n","Try using .loc[row_indexer,col_indexer] = value instead\n","\n","See the caveats in the documentation: https://pandas.pydata.org/pandas-docs/stable/user_guide/indexing.html#returning-a-view-versus-a-copy\n","  filtered[\"tourney_date\"] = pd.to_datetime(filtered[\"tourney_date\"], format=\"%Y%m%d\")\n"]}]},{"cell_type":"code","source":["def compute_player_history(df, n_matches=5):\n","    # Almacenar features generados\n","    rows = []\n","\n","    for idx, row in df.iterrows():\n","        date = row[\"tourney_date\"]\n","        surface = row[\"surface\"]\n","        p1 = row[\"winner_name\"]\n","        p2 = row[\"loser_name\"]\n","        label = 1  # porque p1 ganó\n","\n","        # Historial de cada jugador antes del partido\n","        history_p1 = df[\n","            ((df[\"winner_name\"] == p1) | (df[\"loser_name\"] == p1)) &\n","            (df[\"tourney_date\"] < date)\n","        ].sort_values(\"tourney_date\", ascending=False).head(n_matches)\n","\n","        history_p2 = df[\n","            ((df[\"winner_name\"] == p2) | (df[\"loser_name\"] == p2)) &\n","            (df[\"tourney_date\"] < date)\n","        ].sort_values(\"tourney_date\", ascending=False).head(n_matches)\n","\n","        if len(history_p1) < 3 or len(history_p2) < 3:\n","            continue  # no hay suficiente historial\n","\n","        def win_rate(player, hist):\n","            wins = ((hist[\"winner_name\"] == player).sum())\n","            return wins / len(hist)\n","\n","        # Features básicos por jugador\n","        features = {\n","            \"p1_rank\": row[\"winner_rank\"],\n","            \"p2_rank\": row[\"loser_rank\"],\n","            \"p1_winrate\": win_rate(p1, history_p1),\n","            \"p2_winrate\": win_rate(p2, history_p2),\n","            \"surface\": surface,\n","            \"target\": label\n","        }\n","\n","        rows.append(features)\n","\n","    return pd.DataFrame(rows)\n","\n","# Construir dataset de entrenamiento basado en historial\n","training_df = compute_player_history(matches)\n"],"metadata":{"id":"Yk9Tsbb6W_MO","executionInfo":{"status":"ok","timestamp":1751749189150,"user_tz":300,"elapsed":470414,"user":{"displayName":"Juan José Alzate Molina","userId":"13371847134369819030"}}},"execution_count":7,"outputs":[]},{"cell_type":"code","source":["# Ver las dimensiones del dataset\n","print(f\"Número total de partidos: {training_df.shape[0]}\")\n","print(f\"Número de columnas: {training_df.shape[1]}\")\n","\n","# Mostrar las primeras filas\n","display(training_df.head())\n","\n","# Ver nombres de las columnas\n","print(\"Columnas disponibles:\")\n","print(training_df.columns.tolist())\n","\n","# Ver resumen general\n","display(training_df.describe(include='all'))\n","\n","# Verificar valores nulos por columna\n","print(\"\\nValores nulos por columna:\")\n","print(training_df.isnull().sum())"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":722},"id":"DSGCD-Yta3cJ","executionInfo":{"status":"ok","timestamp":1751749740901,"user_tz":300,"elapsed":41,"user":{"displayName":"Juan José Alzate Molina","userId":"13371847134369819030"}},"outputId":"e830c973-7cc5-4676-c3da-def1ca110182"},"execution_count":11,"outputs":[{"output_type":"stream","name":"stdout","text":["Número total de partidos: 26072\n","Número de columnas: 6\n"]},{"output_type":"display_data","data":{"text/plain":["   p1_rank  p2_rank  p1_winrate  p2_winrate  surface  target\n","0     45.0     81.0    0.666667    0.666667        3       1\n","1     45.0 